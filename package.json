{
  "name": "server",
  "private": true,
  "version": "0.1.0",
  "engines": {
    "node": ">=4.0.0"
  },
  "scripts": {
    "postinstall": "dotnet restore server",
    "start": "node run --debug",
    "test": "node run test --env=jsdom",
    "test:coverage": "node run test --coverage --env=jsdom",
    "tslint": "tslint -c tslint.json 'client/**/*.{ts,tsx}'",
    "clean": "node run clean",
    "build": "node run build",
    "build:debug": "node run build --debug",
    "publish": "node run publish"
  },
  "jest": {
    "transform": {
      "^.+\\.(t|j)sx?$": "ts-jest"
    },
    "collectCoverageFrom": [
      "client/**/*.{js,ts,jsx,tsx,mjs}"
    ],
    "testMatch": [
      "<rootDir>/client/**/__tests__/**/*.{js,ts,jsx,tsx,mjs}",
      "<rootDir>/client/**/?(*.)(spec|test).{js,ts,jsx,tsx,mjs}"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/preset-typescript": "^7.1.0",
    "@types/history": "4.7.0",
    "@types/jest": "^23.3.1",
    "@types/lodash": "^4.14.119",
    "@types/react": "16.7.18",
    "@types/react-dom": "16.0.7",
    "@types/react-hot-loader": "4.1.0",
    "@types/react-redux": "6.0.9",
    "@types/react-router": "4.4.3",
    "@types/react-router-dom": "4.3.1",
    "@types/react-router-redux": "5.0.16",
    "@types/webpack": "4.4.12",
    "@types/webpack-env": "1.13.6",
    "aspnet-prerendering": "^3.0.1",
    "aspnet-webpack": "^3.0.0",
    "awesome-typescript-loader": "5.2.1",
    "babel-loader": "^8.0.2",
    "babel-plugin-styled-components": "^1.10.0",
    "browser-sync": "^2.24.7",
    "cpy": "^7.0.1",
    "css-loader": "1.0.1",
    "del": "^3.0.0",
    "domain-task": "^3.0.3",
    "enzyme": "^3.5.1",
    "enzyme-adapter-react-16": "^1.4.0",
    "eslint": "^5.5.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-react": "^7.11.1",
    "event-source-polyfill": "1.0.5",
    "file-loader": "3.0.1",
    "history": "4.7.2",
    "jest": "^23.5.0",
    "jest-cli": "^23.5.0",
    "lodash": "^4.17.11",
    "mini-css-extract-plugin": "0.5.0",
    "node-noop": "1.0.0",
    "promise": "^8.0.1",
    "prop-types": "^15.6.0",
    "raf": "^3.4.0",
    "react": "16.7.0",
    "react-dev-utils": "7.0.1",
    "react-dom": "16.7.0",
    "react-hot-loader": "4.6.3",
    "react-router-dom": "4.3.1",
    "source-map-loader": "^0.2.4",
    "style-loader": "0.23.1",
    "styled-components": "^4.1.3",
    "ts-jest": "^23.1.4",
    "tslint": "^5.11.0",
    "tslint-react": "^3.5.1",
    "typescript": "3.2.2",
    "typescript-plugin-styled-components": "^1.0.0",
    "url-loader": "1.1.1",
    "webpack": "4.28.3",
    "webpack-cli": "^3.2.0",
    "webpack-dev-middleware": "^3.3.0",
    "webpack-hot-middleware": "2.24.2",
    "webpack-merge": "4.2.1"
  },
  "dependencies": {
    "npm": "^6.4.1"
  }
}
